<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <!-- List View -->
        <record model="ir.ui.view" id="dps_reference_agen_tree">
            <field name="name">dps.reference.agen.tree</field>
            <field name="model">dps.reference.agen</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="kode_reff"/>
                    <field name="uraian"/>
                    <field name="kode_master"/>
                </tree>
            </field>
        </record>

        <!-- Form View -->
        <record model="ir.ui.view" id="dps_reference_agen_form">
            <field name="name">dps.reference.agen.form</field>
            <field name="model">dps.reference.agen</field>
            <field name="arch" type="xml">
                <form>
                    <header>
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="kode_reff"/>
                                <field name="uraian"/>
                                <field name="kode_master" force_save="1" readonly="1"/>
                            </group>
                            <group>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- Action 
            NEGARA
            KOTA
            PROVINSI
            PELABUHAN
            JENIS_IDENTITAS
            UKURAN_CONTAINER
            JENIS_CONTAINER
            UKURAN_KEMASAN
            KATEGORI_BARANG
        -->

        <!-- Action: NEGARA -->
        <record model="ir.actions.act_window" id="dps_reference_agen_negara_action">
            <field name="name">Reference Negara</field>
            <field name="res_model">dps.reference.agen</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('kode_master','=','NEGARA')]</field>
            <field name="context">{'default_kode_master': 'NEGARA'}</field>
        </record>

        <!-- Action: KOTA -->
